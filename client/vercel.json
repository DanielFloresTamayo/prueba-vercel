{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "/dist/proyecto_final/browser"
            }
        }
    ],
    "outputDirectory": "dist/proyecto_final/browser",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}